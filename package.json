{
  "name": "the-leviathans-codex",
  "version": "0.1.0",
  "description": "A collection of D&D resources",
  "main": "index.js",
  "engines": {
    "node": "v14.17.1",
    "npm": "6.14.13"
  },
  "scripts": {
    "install-deps": "npm install && npm install --prefix client",
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG-PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^6.2.1",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "materialize-css": "^1.0.0",
    "mongoose": "^6.0.5",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0"
  }
}
